{
  "name": "@sandbox-apps/ai",
  "version": "1.0.0",
  "description": "Shared AI package with OpenAI agents SDK for sandbox apps",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "clean": "rm -rf dist"
  },
  "keywords": ["ai", "openai", "agents", "llm"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@openai/agents": "^0.1.1",
    "openai": "^4.52.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
